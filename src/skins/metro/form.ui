  <property name="styleSheet">
   <string notr="true">#titleLabel {
	color : #FFFFFF;
}

#borderWidget {
	background-color: #5882CF;
}

#coverWidget {
	background: transparent;
	border: 1px solid #252526;
}
#coverWidget:hover {
	border: 1px solid #5882CF;
}

#splitter {
	background: #2D2D30; /* also main skin background */
}
#splitter::handle {
	background-image: url(splitter.png);
	background-repeat: no-repeat;
	background-position: center;
	padding: 1 0;
}

#playlistWidget {
	color: #D4D4D4;
	qproperty-current_text_color: #FFFFFF;
	qproperty-failed_text_color: #565659;
	background: #252526;
	padding: 5;
}
#playlistWidget::item {
	background: #252526;
	border: 1 solid transparent;
}
#playlistWidget::item:alternate {
	background: #2D2D30;
}
#playlistWidget::item:selected {
	background: #2C3A52;
	border: 1 solid #4465A1;
}
#playlistWidget::item:alternate:selected {
	background: #343F52;
}
#playlistWidget::item:hover {
	border: 1 solid #5882CF;
}
#playlistWidget::item:selected:hover {
	background: #2B4169;
}
#playlistWidget::item:alternate:selected:hover {
	background: #3B4B69;
}

#playlistWidget QScrollBar {
	background: transparent;
	width: 15;
	margin-bottom: 15;
}
#playlistWidget QScrollBar::handle {
	background: #434346;
	margin-left: 5;
	min-height: 30px;
}
#playlistWidget QScrollBar::handle:hover {
	background: #6798F2;
}
#playlistWidget QScrollBar::add-line,
#playlistWidget QScrollBar::sub-line,
#playlistWidget QScrollBar::add-page,
#playlistWidget QScrollBar::sub-page {
	height: 0px;
	background: transparent;
}
#playlistWidget QSlider::sub-page:horizontal {
	background: #757575;
}

#mainWindow &gt; QSizeGrip, #sizeGrip {
	background-image: url(sizegrip.png);
	background-repeat: none;
	width: 17px;
	height: 17px;
}

#iconLabel {
	border-image: url(icon.png);
}

#shuffleCheckBox {
	qproperty-icon: url(shuffle.png);
}
#repeatCheckBox {
	qproperty-icon: url(repeat.png);
}
#repeatCheckBox, #shuffleCheckBox {
	border: 1 solid transparent;
}
#repeatCheckBox:checked, #shuffleCheckBox:checked {
	background: #4173D1;
}
#prevButton, #playButton, #stopButton, #nextButton {
	background: transparent;
}
#prevButtonWrapper, #playButtonWrapper,
#stopButtonWrapper, #nextButtonWrapper {
	border: 2px solid #FFFFFF;
	border-radius: 15px;
	margin: 3px 15px;
}
#playButton:hover, #stopButton:hover,
#nextButton:hover, #prevButton:hover,
#repeatCheckBox:hover, #shuffleCheckBox:hover,
#minimizeButton:hover, #themeButton:hover {
	border: 1 solid #5882CF;
}

#playButton:pressed, #stopButton:pressed,
#nextButton:pressed, #prevButton:pressed,
#repeatCheckBox:pressed, #shuffleCheckBox:pressed,
#minimizeButton:pressed, #themeButton:pressed {
	background: rgba(55, 99, 179, 80);
	padding: 0;
}
#prevButton {
	qproperty-icon: url(prev.png);
}
#stopButton {
	qproperty-icon: url(stop.png);
}
#playButton {
	qproperty-icon: url(play.png);
}
#nextButton {
	qproperty-icon: url(next.png);
}
#closeButton {
	qproperty-icon: url(close.png);
}
#minimizeButton {
	qproperty-icon: url(minimize.png);
}
#themeButton {
	qproperty-icon: url(theme.png);
}

#minimizeButton, #closeButton, #themeButton {
	border: 0;
}
#closeButton:pressed {
	background: #781212;
	padding: 0;
}
#closeButton:hover {
	border: 1 solid #FF2E12;
}

#volumeSlider::groove:horizontal {
	background: #434346;
	height: 10;
}
#volumeSlider::handle:horizontal {
	width: 20;
	background: #FFFFFF;
}
#volumeSlider::handle:horizontal:hover {
	background: #6798F2;
}
#volumeSlider::sub-page:horizontal {
	background: #5D5D61;
}

NTrackInfoWidget QLabel {
	color: #FFFFFF;
	max-height: 14px;
	font-size: 11px;
	font-weight: bold;

	background: argb(37, 37, 38, 200);
	padding: 0 1;
}

NTrackInfoWidget #MiddleCenter {
	font-size: 13px;
	padding-bottom: 2;
}

#waveformSlider {
	qproperty-radius: 0;
	background: #252526;
	background-image: url(grid.png);
	qproperty-background: #252526;
	qproperty-wave_background: #1B58B8;
	qproperty-wave_border_color: #1FAEFF;
	qproperty-progress_normal_background: #15992A;
	qproperty-progress_paused_background: #CE8419;
	qproperty-normal_composition: Overlay;
}</string>
